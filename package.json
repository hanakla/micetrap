{
  "name": "micetrap",
  "version": "1.0.0-beta.1",
  "description": "Simple library for handling keyboard shortcuts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/hanak1a/mousetrap.git"
  },
  "author": "Hanakla <hanakla.dev@gmail.com>",
  "license": "Apache-2.0",
  "readmeFilename": "README.md",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "scripts": {
    "ci": "npm run clean && npm run lint && npm run build:prod",
    "build": "npm run clean && tsup src/index.ts",
    "dev": "npm run clean && tsup src/index.ts --watch",
    "clean": "npm run clean:dist && npm run clean:cache",
    "clean:dist": "rimraf -- dist",
    "clean:cache": "rimraf -- .tmp",
    "precommit": "",
    "prepush": "npm run build"
  },
  "devDependencies": {
    "@types/react": "^19.0.7",
    "happy-dom": "^16.7.2",
    "husky": "0.14.3",
    "react": "^19.0.0",
    "rimraf": "2.6.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^3.0.3"
  }
}
